# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/workflow_automation
DB_HOST=localhost
DB_PORT=5432
DB_NAME=workflow_automation
DB_USER=postgres
DB_PASSWORD=your_password

# YouTube API Keys (add multiple for rotation)
YOUTUBE_API_KEY_1=your_youtube_api_key_here
YOUTUBE_API_KEY_2=your_second_api_key_optional
YOUTUBE_API_KEY_3=your_third_api_key_optional
YOUTUBE_API_QUOTA_LIMIT=10000

# Scraping Configuration
MAX_VIDEOS_PER_TERM=100
MAX_CONCURRENT_REQUESTS=10
MIN_VIDEO_VIEWS=500
MIN_VIDEO_DURATION=120
MAX_VIDEO_DURATION=7200
DATE_FILTER_YEARS=5

# Quality Scoring
MIN_QUALITY_SCORE=50
ENABLE_AUTO_CLASSIFICATION=true
ENABLE_SERIES_DETECTION=true

# Performance
BATCH_INSERT_SIZE=50
ENABLE_PARALLEL_SCRAPING=true
MAX_WORKERS=5

# Application Settings
LOG_LEVEL=INFO
LOG_FILE=scraper.log





